	package my.jes.web.dao;

import java.util.List;

import org.apache.ibatis.session.SqlSession;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Repository;

import my.jes.web.vo.MemberVO;

@Repository
public class MemberDAO {
	
	@Autowired
	SqlSession sqlSession;
	
	public void memberInsert(MemberVO m) {
		sqlSession.insert("mapper.member.memberInsert", m);
	}

//	public String login(MemberVO m) {		
//		return sqlSession.selectOne("mapper.member.login", m);
//	}
	
	public List<Object> login(MemberVO m) {
		
		List<Object> list = sqlSession.selectList("mapper.member.login", m);
		//[MemberVO [id=a, pw=asdf!!!, name=a, email=a@gmail.com, gender=male, age=27, weight=63.7, height=173.6]]
		for (Obj obj : list) {
			//MemberVO [id=a, pw=asdf!!!, name=a, email=a@gmail.com, gender=male, age=27, weight=63.7, height=173.6]
			for (int j = 0; j < list.get(0).toString().length(); j++) {
				System.out.println(list.get(0).toString());
			}
		}
		return list;
	}

	public void deleteMember(MemberVO m) {
		sqlSession.delete("mapper.member.deleteMember", m.getId());
		
	}

	public List<Object> getCal(MemberVO m) {
		List<Object> list = sqlSession.selectList("mapper.member.getCal", m.getId());
		return list;
	}

}
